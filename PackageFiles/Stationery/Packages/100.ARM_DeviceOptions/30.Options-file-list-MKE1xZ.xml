<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0">
   <!-- Extra Optional files for MKE1xZ -->
   <projectActionList id="Options-file-list-MKE1xZ">
      <applyWhen>
         <and>
            <deviceSubfamilyIs>MKE1xZ7</deviceSubfamilyIs>
            <variableRef idRef="outputType" condition="matches" value=".*ProjectType\.exe$" />
         </and>
      </applyWhen>
      <!-- Clock -->
      <copy source="Startup_Code/clock-SCG_MKE1xZ7.c"              target="Startup_Code/clock.c"              overwrite="always" />
      <copy source="Project_Headers/clock_configure-SCG-MKE1xZ7.h" target="Project_Headers/clock_configure.h" overwrite="always" />
      <!-- Security with boot -->
      <copy source="Startup_Code/security-mke-boot.c"   target="Startup_Code/security.c" overwrite="always" />
      <!-- Console -->
      <copy source="Startup_Code/console-mke-lp.c" target="Startup_Code/console.c"    overwrite="always" />
      <copy source="Project_Headers/console.h"     target="Project_Headers/console.h" overwrite="always" />
   </projectActionList>
</root>