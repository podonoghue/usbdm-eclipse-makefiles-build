<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0">
   <!-- Extra Optional files for MKE1xF -->
   <projectActionList id="Options-file-list-MKE1xF">
      <applyWhen>
         <and>
            <deviceSubfamilyIs>MKE1xF16</deviceSubfamilyIs>
            <variableRef idRef="outputType" condition="matches" value=".*ProjectType\.exe$" />
         </and>
      </applyWhen>
      <!-- Clock no support for System Clock Generator (SCG) -->
      <!-- Security with boot -->
      <copy source="Startup_Code/security-mke-boot.c"   target="Startup_Code/security.c" overwrite="always" />
      <!-- Console -->
      <copy source="Startup_Code/console-mk.c" target="Startup_Code/console.c"    overwrite="always" />
      <copy source="Project_Headers/console.h" target="Project_Headers/console.h" overwrite="always" />
   </projectActionList>
</root>