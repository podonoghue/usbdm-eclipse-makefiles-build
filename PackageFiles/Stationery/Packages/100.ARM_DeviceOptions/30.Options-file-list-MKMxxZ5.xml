<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0">
   <!-- Extra Optional files for MKMxx -->
   <projectActionList id="Options-file-list-MKMxxZ5">
      <applyWhen>
         <and>
            <or>
               <deviceSubfamilyIs>MKM14Z5</deviceSubfamilyIs>
               <deviceSubfamilyIs>MKM33Z5</deviceSubfamilyIs>
               <deviceSubfamilyIs>MKM34Z5</deviceSubfamilyIs>
            </or>
            <variableRef idRef="outputType" condition="matches" value=".*ProjectType\.exe$" />
         </and>
      </applyWhen>
      <copy source="Startup_Code/vectors-cm0.c"         target="Startup_Code/vectors.c"  overwrite="always" />
      <!-- Security -->
      <copy source="Startup_Code/security-mkm.c"        target="Startup_Code/security.c" overwrite="always" />
      <!-- Clock -->
      <copy source="Startup_Code/clock-MCG-MKMxx.c"              target="Startup_Code/clock.c"              overwrite="always" />
      <copy source="Project_Headers/clock_configure-MCG-MKMxx.h" target="Project_Headers/clock_configure.h" overwrite="always" />
      <!-- Console -->
      <copy source="Startup_Code/console-mkm.c"    target="Startup_Code/console.c"    overwrite="always" />
      <copy source="Project_Headers/console.h"     target="Project_Headers/console.h" overwrite="always" />
   </projectActionList>
</root>