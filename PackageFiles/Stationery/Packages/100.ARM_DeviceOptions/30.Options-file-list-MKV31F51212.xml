<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0">
   <!-- Extra Optional files for MKV31F51212 -->
   <projectActionList id="Options-file-list-MKV31F51212">
      <applyWhen>
         <and>
            <deviceSubfamilyIs>MKV31F51212</deviceSubfamilyIs>
            <variableRef idRef="outputType" condition="matches" value=".*ProjectType\.exe$" />
         </and>
      </applyWhen>
      <copy source="Startup_Code/vectors-cm0.c"         target="Startup_Code/vectors.c"  overwrite="always" />
      <!-- Security -->
      <copy source="Startup_Code/security-mkv31.c"        target="Startup_Code/security.c" overwrite="always" />
      <!-- Clock -->
      <copy source="Startup_Code/clock-MCG-MKV31F512M12.c"              target="Startup_Code/clock.c"              overwrite="always" />
      <copy source="Project_Headers/clock_configure-MCG-MKV31F512M12.h" target="Project_Headers/clock_configure.h" overwrite="always" />
      <!-- Console -->
      <copy source="Startup_Code/console-mk.c" target="Startup_Code/console.c"    overwrite="always" />
      <copy source="Project_Headers/console.h" target="Project_Headers/console.h" overwrite="always" />
   </projectActionList>

</root>