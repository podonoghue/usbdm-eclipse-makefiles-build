<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0">
   <!-- Extra Optional files for MKxxM12/M15 with Dual PLLs -->
   <projectActionList id="Options-file-list-MKxxM12-DualPLL">
      <applyWhen>
         <and>
            <or>
               <deviceSubfamilyIs>-----</deviceSubfamilyIs>
            </or>
            <variableRef idRef="outputType" condition="matches" value=".*ProjectType\.exe$" />
         </and>
      </applyWhen>
      <!-- Clock -->
      <copy source="Startup_Code/clock-MCG-MKxxM12-DualPLL.c"              target="Startup_Code/clock.c"              overwrite="always" />
      <copy source="Project_Headers/clock_configure-MCG-MKxxM12-DualPLL.h" target="Project_Headers/clock_configure.h" overwrite="always" />
      <!-- Console -->
      <copy source="Startup_Code/console-mk.c" target="Startup_Code/console.c"    overwrite="always" />
      <copy source="Project_Headers/console.h" target="Project_Headers/console.h" overwrite="always" />
   </projectActionList>
</root>