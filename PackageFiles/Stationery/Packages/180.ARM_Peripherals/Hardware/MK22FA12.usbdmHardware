      <peripheral baseName="FTM"      instance="1"        instanceList="FTM0;FTM1;FTM2;FTM3" version="ftm1_2ch_quad">
         <handler class="net.sourceforge.usbdm.deviceEditor.peripherals.WriterForFtm" />
         <clock clockEnable="SIM->SCGC6 = SIM->SCGC6 | SIM_SCGC6_FTM1_MASK;" clockDisable="SIM->SCGC6 = SIM->SCGC6 &amp; ~SIM_SCGC6_FTM1_MASK;" />
         <irq num="FTM1_IRQn" />
         <param type="StringVariable"         name="_instanceList"                  value="FTM0;FTM1;FTM2;FTM3" key="/FTM1/_instanceList" />
         <param type="LongVariable"           name="_hardwareIrqCount"              value="1"                key="/FTM1/_hardwareIrqCount" />
         <param type="StringVariable"         name="_hardwareIrqNums"               value="FTM1_IRQn"        key="/FTM1/_hardwareIrqNums" />
         <param type="BooleanVariable"        name="_hasPcrTable"                   value="true"             key="/FTM1/_hasPcrTable" />
         <param type="BooleanVariable"        name="_present"                       value="true"             key="/FTM1/_present" />
         <param type="BooleanVariable"        name="ftm_cnsc_chf_present"           value="true"             key="/FTM1/ftm_cnsc_chf_present" />
