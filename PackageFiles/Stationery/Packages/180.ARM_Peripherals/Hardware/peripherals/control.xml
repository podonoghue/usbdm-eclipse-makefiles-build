<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE peripheralPage SYSTEM "_menu.dtd" >
<!-- control.xml -->

<peripheralPage xmlns:xi="http://www.w3.org/2001/XInclude" name="_instance" description="Control">

   <!-- ************* Signal mapping ****************** -->
   <category name="SIM" description="Pin mapping">
      <aliasOption key="/SIM/sim_scgc_swd"      constant="false" optional="true" />
      <for keys="reg" values="sopt;sopt0" >
         <aliasOption key="/SIM/sim_%(reg)_swde"    constant="false" optional="true" />
         <aliasOption key="/SIM/sim_%(reg)_rstpe"   constant="false" optional="true" />
         <aliasOption key="/SIM/sim_%(reg)_nmie"    constant="false" optional="true" />
         <aliasOption key="/SIM/sim_%(reg)_busref"  constant="false" optional="true" />
      </for>
   </category>

   <signals/>

   <projectActionList id = "control_files" >
      <copy source="Project_Headers/control.h"          target="Project_Headers/control.h"          overwrite="true" derived="true" />
   </projectActionList>
</peripheralPage>
