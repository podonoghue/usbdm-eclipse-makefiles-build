#!/bin/bash

HOME_DIR="$(dirname $(readlink -f $0))"
echo "Working in "${HOME_DIR}""
cd ${HOME_DIR}

exit

echo starting
rm -r new
mkdir new

for filename in *.xml; do
   echo sed  's/constant=/locked=/g' "${filename}" "new/${filename}"
   sed  's/constant=/locked=/g' "${filename}" >"new/${filename}"
done

