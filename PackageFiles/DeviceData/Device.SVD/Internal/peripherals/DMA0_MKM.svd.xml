                           </enumeratedValue>
                           <enumeratedValue>
                              <name>0b1X</name>
                              <description>1x Read + Write - (none) - (none)</description>
                              <value>0b1X</value>
                           </enumeratedValue>
                        </enumeratedValues>
                     </field>
