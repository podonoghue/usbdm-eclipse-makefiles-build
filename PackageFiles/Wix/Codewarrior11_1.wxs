         <RegistrySearch Id="Rs.CW_FOR_MCU_$(var.CW_VER_UC)"
                    Root="HKLM"
                    Win64="no"
                    Key="SOFTWARE\Freescale\CodeWarrior\Product Versions\CW for MCU $(var.CW_Ver)"
                    Name="Path"
                    Type="directory" />
      </Property>

      <!-- Modify default install path for CodeWarrior Development Studio for MCU V10.x -->
